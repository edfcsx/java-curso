package fundamentals;

public class Concat {

	public static void main(String[] args) {
		String nome = "Maria";
		int idade = 31;
		double renda = 3000.0;
		
		System.out.println("A forma mais comum de concatenação usando" + " o operador +");
		System.out.printf("Variavel %f, Variavel %f", 1.0, 2.0);
		System.out.printf("\n%s tem $d anos e uma renda de %.2f", nome, idade, renda);
	}

}
